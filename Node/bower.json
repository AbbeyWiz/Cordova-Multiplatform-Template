{
  "name": "App",
  "dependencies": {
    "ionic": "1.0.0-rc.2",
    "DefinitelyTyped": "*"
  },
  "overrides": {
    "ionic": {
      "main": [
        "**/ionic.bundle.js",
        "**/ionic.css",
        "**/release/fonts/**.*"
      ]
    },
    "angular": {
      "ignore": true
    },
    "angular-animate": {
      "ignore": true
    },
    "angular-sanitize": {
      "ignore": true
    },
    "angular-ui-router": {
      "ignore": true
    },
    "DefinitelyTyped": {
      "main": [
        "angularjs/angular.d.ts",
        "jquery/jquery.d.ts",
        "ionic/ionic.d.ts",
        "angular-ui-router/angular-ui-router.d.ts",
        "cordova/**/*.ts",
        "cordova-ionic/**/*.ts"
      ]
    }
  },
  "resolutions": {
    "angular": ">= 1.1.5",
    "angular-animate": "1.3.13",
    "angular-sanitize": "1.3.13",
    "angular-ui-router": "0.2.13"
  }
}
